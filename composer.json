{
	"name": "nette/web-project",
	"description": "Nette: Standard Web Project",
	"keywords": ["nette"],
	"type": "project",
	"license": ["MIT", "BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"require": {
		"php": ">= 8.4",
		"ext-mbstring": "*",
		"ext-intl": "*",
		"ext-pdo": "*",
		"nette/application": "^3.2.3",
		"nette/assets": "^1.0.0",
		"nette/bootstrap": "^3.2.6",
		"nette/database": "^3.2",
		"nette/di": "^3.2",
		"nette/forms": "^3.2",
		"nette/http": "^3.3",
		"nette/mail": "^4.0",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.2",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.10"
    },
	"require-dev": {
		"nette/tester": "^2.5",
		"symfony/thanks": "^1",
		"phpstan/phpstan": "^2.1"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app",
			"Bite\\": "bite"
		}
	},
	"minimum-stability": "stable",
	"config": {
		"allow-plugins": {
			"symfony/thanks": true
		}
	},
  "scripts": {
    "phpstan": "php -d memory_limit=512M vendor/bin/phpstan analyse -c phpstan.neon"
  }
}
